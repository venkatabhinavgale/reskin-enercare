{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "portent/accordion",
	"version": "0.5.0",
	"title": "Mobile Accordion Panel",
	"category": "layout",
	"icon": "image-flip-vertical",
	"keywords": [ "faq", "accordion", "collapsible" ],
	"description": "A custom accordion block that is responsive and accessible. ",
	"supports": {
		"html": false
	},
	"attributes": {
		"accordionTitle": {
			"type": "string"
		},
		"accordionID": {
			"type": "string"
		},
		"titleLevel": {
			"type": "integer",
			"default": 2
		},
		"tagName" : {
			"type": "string",
			"default": "h2"
		}
	},
	"textdomain": "portent-accordion",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
